meta {
  name: page
  type: http
  seq: 5
}

get {
  url: {{host}}/api/pages?filters[slug]=search&populate[content][on][collection.search-bar]=*&pagination[limit]=1
  body: none
  auth: bearer
}

params:query {
  filters[slug]: search
  populate[content][on][collection.search-bar]: *
  pagination[limit]: 1
}

auth:bearer {
  token: {{token}}
}

settings {
  encodeUrl: true
  timeout: 0
}
